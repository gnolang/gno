package awesomegno

import (
	"std"
	"time"

	"gno.land/p/demo/avl"
	"gno.land/p/demo/seqid"
)

type Category struct {
	ID          seqid.ID
	Name        string
	Description string
	CreatedAt   time.Time
	items       *avl.Tree
}

type Item struct {
	ID          seqid.ID
	Title       string
	Description string
	Authors     []std.Address
	URL         string
	CreatedAt   time.Time
	categories  *avl.Tree
}
