package main

func main() {
	var fns []func() int

	for i := 0; i < 2; i++ {
		x := i
		y := 1
		b := true
		f := func() int {
			switch y {
			case 1:
				if b {
					z := 5
					for j := 0; j < z; j++ {
						x += 1
					}
				}
			default:
				x += 0
			}
			return x
		}
		fns = append(fns, f)
	}
	for _, fn := range fns {
		println(fn())
	}
}

// Output:
// 5
// 6
