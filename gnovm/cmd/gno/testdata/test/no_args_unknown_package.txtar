# Test for loading an unknown package without path arguments

! gno test -v

! stdout .+
stderr 'contract.gno:3:8: unknown import path foobarbaz'

-- contract.gno --
package contract

import "foobarbaz"

func Foo() {
	_ = foobarbaz.Gnognogno
}

-- contract_test.gno --
package contract

import "testing"

func TestFoo(t *testing.T) {
	Foo()
}
