-- run_test.gno --
package run

import (
	"testing"
)

func TestRun(t *testing.T) {
	cases := []string {
		"hello",
		"hi you",
		"hi me",
	}
	for _, tc := range cases {
		t.Run(tc, func(t *testing.T) {})
	}
}



