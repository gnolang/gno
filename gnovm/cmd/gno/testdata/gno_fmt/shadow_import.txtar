# Test error parsing

gno fmt -include v1 -include v2 v2/file_filetest.gno
cmp stdout stdout.golden
cmp stderr stderr.golden

-- v1/file.gno --
package v1

func Get(v string) string {
	return "v1:"+ v
}

-- v1/gno.mod --
module "gno.land/r/dev/shadow/v1"

-- v2/file.gno --
package v1

func Get(v string) string {
	return "v2:"+ v
}


-- v2/file_filetest.gno --
package main

import (
	// should be valid
	"gno.land/r/dev/shadow/v2"
)

func main() {
	println("a", v1.Get("a"))
}

-- v2/gno.mod --
module "gno.land/r/dev/shadow/v2"


-- stdout.golden --
package main

import (
	// should be valid
	"gno.land/r/dev/shadow/v2"
)

func main() {
	println("a", v1.Get("a"))
}
-- stderr.golden --
