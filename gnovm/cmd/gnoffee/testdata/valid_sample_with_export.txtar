# Test with a valid sample.gnoffee

gnoffee -w .

! stderr .+ 
! stdout .+

cmp gen.golden gnoffee.gen.go

-- sample.gnoffee --
package sample

type foo struct{}

export baz as Bar

var baz = foo{}

func (f *foo) Hello() string {
	return "Hello from foo!"
}

func (f *foo) Bye() {
	println("Goodbye from foo!")
}

type Bar interface {
	Hello() string
  Bye()
}

