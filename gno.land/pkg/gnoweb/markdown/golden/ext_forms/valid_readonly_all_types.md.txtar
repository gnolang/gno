-- input.md --
<gno-form>
<gno-input name="username" type="text" value="alice" readonly="true" />
<gno-input name="age" type="number" value="25" readonly="true" />
<gno-input name="email" type="email" value="alice@example.com" readonly="true" />
<gno-input name="gender" type="radio" value="female" checked="true" readonly="true" />
<gno-input name="gender" type="radio" value="male" readonly="true" />
<gno-input name="newsletter" type="checkbox" value="subscribed" checked="true" readonly="true" />
<gno-textarea name="bio" value="This is my bio" readonly="true" />
<gno-select name="role" value="admin" selected="true" readonly="true" />
<gno-select name="role" value="user" readonly="true" />
</gno-form>

-- output.html --
<form class="gno-form" method="post" action="/r/test" autocomplete="off" spellcheck="false">
<div class="gno-form_header">
<span><span class="font-bold">/r/test</span> Form</span>
<span class="tooltip" data-tooltip="Processed securely by /r/test"><svg class="w-3 h-3"><use href="#ico-info"></use></svg></span>
</div>
<div class="gno-form_input"><label for="username">Enter value<span class="gno-form_info-badge">(readonly)</span></label>
<input type="text" id="username" name="username" placeholder="Enter value" value="alice" readonly />
</div>
<div class="gno-form_input"><label for="age">Enter value<span class="gno-form_info-badge">(readonly)</span></label>
<input type="number" id="age" name="age" placeholder="Enter value" value="25" readonly />
</div>
<div class="gno-form_input"><label for="email">Enter value<span class="gno-form_info-badge">(readonly)</span></label>
<input type="email" id="email" name="email" placeholder="Enter value" value="alice@example.com" readonly />
</div>
<div class="gno-form_selectable">
<input type="radio" id="gender_3" name="gender" value="female" checked disabled />
<label for="gender_3">female<span class="gno-form_info-badge">(readonly)</span></label>
</div>
<div class="gno-form_selectable">
<input type="radio" id="gender_4" name="gender" value="male" disabled />
<label for="gender_4">male<span class="gno-form_info-badge">(readonly)</span></label>
</div>
<div class="gno-form_selectable">
<input type="checkbox" id="newsletter_5" name="newsletter" value="subscribed" checked disabled />
<label for="newsletter_5">subscribed<span class="gno-form_info-badge">(readonly)</span></label>
</div>
<div class="gno-form_input"><label for="bio">Enter value<span class="gno-form_info-badge">(readonly)</span></label>
<textarea id="bio" name="bio" placeholder="Enter value" rows="4" readonly>This is my bio</textarea>
</div>
<div class="gno-form_select"><label for="role">Role<span class="gno-form_info-badge">(readonly)</span></label>
<select id="role" name="role" disabled>
<option value="">Select a Role</option>
<option value="admin" selected>admin</option>
<option value="user">user</option>
</select>
<svg class="w-4 h-4 absolute right-2 top-1/2 -translate-y-1/2 pointer-events-none"><use href="#ico-arrow"></use></svg>
</div>
<div class="gno-form_input"><input type="submit" value="Submit to /r/test Realm" /></div>
</form>
