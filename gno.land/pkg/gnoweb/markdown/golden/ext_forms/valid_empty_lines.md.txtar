-- input.md --
<gno-form>
<gno-input name="email" type="email" placeholder="Enter your email" />

<gno-input name="password" type="password" placeholder="Enter your password" />
<gno-input name="age" type="number" placeholder="Enter your age" />


<gno-input name="username" type="text" placeholder="Enter your username" />

<gno-input name="default" placeholder="Default type (text)" />
<gno-input name="invalid" type="invalid" placeholder="Invalid type (should be text)" />
</gno-form>

-- output.html --
<form class="gno-form" method="post" action="/r/test" autocomplete="off" spellcheck="false">
<div class="gno-form_header">
<span><span class="font-bold">/r/test</span> Form</span>
<span class="tooltip" data-tooltip="Processed securely by /r/test"><svg class="w-4 h-4"><use href="#ico-info"></use></svg></span>
</div>
<div class="gno-form_input"><label for="email"> Enter your email </label>
<input type="email" id="email" name="email" placeholder="Enter your email" />
</div>
<div class="gno-form_input"><label for="password"> Enter your password </label>
<input type="password" id="password" name="password" placeholder="Enter your password" />
</div>
<div class="gno-form_input"><label for="age"> Enter your age </label>
<input type="number" id="age" name="age" placeholder="Enter your age" />
</div>
<div class="gno-form_input"><label for="username"> Enter your username </label>
<input type="text" id="username" name="username" placeholder="Enter your username" />
</div>
<div class="gno-form_input"><label for="default"> Default type (text) </label>
<input type="text" id="default" name="default" placeholder="Default type (text)" />
</div>
<!-- Error: invalid input type -->
<input type="submit" value="Submit to /r/test Realm" />
</form>
