stdout 'TX HASH:    '

gnokey maketx call -pkgpath gno.land/r/demo/ee -func Bar -gas-fee 1000000ugnot -gas-wanted 2000000 -broadcast -chainid=tendermint_test test1
stdout OK!
stdout 'GAS WANTED: 2000000'
stdout 'GAS USED:   \d+'
stdout 'HEIGHT:     \d+'
stdout 'EVENTS:     \[{\"type\":\"bar\",\"attrs\":\[{\"key\":\"foo\",\"value\":\"bar\"}\],\"pkg_path\":\"gno.land/r/demo/ee\"}\]'
stdout 'TX HASH:    '

-- ee.gno --
package ee

import "chain"

const (
	EventFoo = "foo"
	EventBar = "bar"
)

func Foo(cur realm) {
	SubFoo()
	SubBar()
}

func SubFoo() {
	chain.Emit(
		EventFoo,
		"key1", "value1",
		"key2", "value2",
		"key3", "value3",
	)
}

func SubBar() {
	chain.Emit(
		EventBar,
		"bar", "baz",
	)
}

func Bar(cur realm) {
	chain.Emit(
		EventBar,
		"foo", "bar",
	)
}
