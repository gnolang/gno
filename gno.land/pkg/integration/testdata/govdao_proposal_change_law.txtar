package create_proposal

import (
	"gno.land/r/gov/dao"
	"gno.land/r/gov/dao/v3/impl"
)

func init() {
	preq := impl.NewChangeLawRequest(&impl.Law{Supermajority:50})
	dao.MustCreateProposal(preq)
}

