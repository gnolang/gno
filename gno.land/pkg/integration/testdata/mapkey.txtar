-- realm.gno --
package main

import "strconv"

type stringMap map[int]int

var (
	key int = 1
	accounts = stringMap{}
)

func Register(cur realm) {
	accounts[key] = 1
}

func Set(cur realm, v int) {
	key = v
}

func Render(cur realm) string {
	i := 0
	s := ""
	for k := range accounts {
		s += ""+ strconv.Itoa(i) + "key.num:"+ strconv.Itoa(k)
		i++
	}
	return s
}
