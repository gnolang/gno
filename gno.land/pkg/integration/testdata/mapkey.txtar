-- realm.gno --
package main

import (
	"strconv"
)

type myInt struct {
	num int
}

type stringMap map[myInt]int

var (
	accounts = stringMap{}
	tmpInt   = myInt{1}
)

func Register() {
	crossing()
	accounts[tmpInt] = 1
}

func Set(i int) {
	crossing()
	tmpInt.num = i
}

func Render(string) string {
    crossing()
	s := ""
	i := 0
	for k := range accounts {
		s += ""+ strconv.Itoa(i) + "key.num:"+ strconv.Itoa(k.num) + "\n"
		i++
	}
	return s
}