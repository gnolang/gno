# load a package from the 'examples' and $WORK directory.
loadpkg gno.land/p/demo/ufmt
loadpkg gno.land/r/importtest $WORK

## start a new node
gnoland start

## execute Render
gnokey maketx call -pkgpath gno.land/r/importtest@v0.0.0 -func Render -gas-fee 1000000ugnot -gas-wanted 2000000 -args '' -broadcast -chainid=tendermint_test test1
stdout '("92054" string)'
stdout OK!

-- gno.mod --
module gno.land/r/importtest v0.0.0

require gno.land/p/demo/ufmt v0.0.1

-- import.gno --
package importtest

import (
	"gno.land/p/demo/ufmt"
)

func Render(_ string) string {
 	return ufmt.Sprintf("%d", 92054)
}

