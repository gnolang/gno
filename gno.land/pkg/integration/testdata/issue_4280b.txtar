-- realm.gno --
package main

import (
	"strconv"
)

type myInt struct {
	num int
	myint2 MyInt2
}

type MyInt2 struct{
    num2 int
}

type stringMap map[myInt]int

var (
	accounts = stringMap{}
	tmpInt   = myInt{num:1, myint2: MyInt2{num2: 2}}
)

func Register(_ realm) {
	accounts[tmpInt] = 1
}

func Render(_ realm, s string) string {
	return "[Break the map!](maperror$help&func=Register)\n\nmap len: " + strconv.Itoa(len(accounts))
}
