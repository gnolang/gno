-- realm.gno --
package main

import "strconv"

type myInt struct {
	num int
}

type stringMap map[int]myInt

var (
	accounts = stringMap{}
	tmpInt   = myInt{1}
)

func Register(_ realm) {
	accounts[1] = tmpInt
}

func Render(_ realm, s string) string {
    return strconv.Itoa(accounts[1].num)
}
