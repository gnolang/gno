-- proposer/create_fee_proposal.gno --
package main

import (
	"gno.land/r/gov/dao"
	"gno.land/r/gov/dao/v3/impl"
)

func main() {
	preq := impl.NewProposalFeeAmountRequest(2000000, "Increasing fee to reduce spam")
	dao.MustCreateProposal(cross, preq)
}

