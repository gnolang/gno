loadpkg gno.land/r/aaa/bbb/ccc $WORK
loadpkg gno.land/r/aaa/ccc/ddd $WORK
loadpkg gno.land/r/aaa $WORK
loadpkg gno.land/r/bbb/ccc $WORK
loadpkg gno.land/p/bbb/ccc/ppp $WORK
loadpkg gno.land/r/bbb/aaa $WORK
