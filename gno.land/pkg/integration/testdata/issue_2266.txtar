-- realm.gno --
package main

type A struct {
	A string
}
type B struct {
	A *A
	B string
}

var (
	a = &A{A: "here"}
	b [2]*B
)

func Fill(cur realm, i int) {
	c := B{
		A: a,
		B: "",
	}
	b[i] = &c
}

func UnFill(cur realm) {
	b[0] = nil
	b[1] = nil
}
