package cbee

import (
    "std"
)

const (
	foo = "foo"
	bar = "bar"
)

type contractA struct{}

func (c *contractA) foo(cb func()) {
	subFoo()
	cb()
}

func subFoo() {
	std.Emit(foo, "k1", "v1", "k2", "v2")
}

type contractB struct{}

func (c *contractB) subBar() {
	std.Emit(bar, "bar", "baz")
}

func Foo() {
    a := &contractA{}
	b := &contractB{}

	a.foo(func() {
		b.subBar()
	})
}